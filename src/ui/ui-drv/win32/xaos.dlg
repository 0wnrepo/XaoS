  EDITTEXT 65, 132, 159, 76, 11, ES_AUTOHSCROLL | WS_TABSTOP
  DEFPUSHBUTTON "&OK", 1, 2, 173, 66, 14, WS_GROUP
  PUSHBUTTON "&Cancel", 2, 72, 173, 66, 14
  PUSHBUTTON "&Help", 3, 143, 173, 66, 14
