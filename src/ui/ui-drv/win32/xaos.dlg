  COMBOBOX 50, 132, 117, 76, 110, CBS_DROPDOWNLIST | CBS_HASSTRINGS | WS_VSCROLL | WS_TABSTOP
  RTEXT "Always recalculate:", 145, 0, 131, 128, 11, WS_GROUP
  COMBOBOX 55, 132, 131, 76, 110, CBS_DROPDOWNLIST | CBS_HASSTRINGS | WS_VSCROLL | WS_TABSTOP
